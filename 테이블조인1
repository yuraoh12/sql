# JOIN
# 주문 테이블과 회원 테이블 붙이기
SELECT *
FROM t_order
INNER JOIN t_user
ON t_order.userNo = t_user.id;

# 주문 테이블과 상품 테이블 붙이기
SELECT *
FROM t_order
INNER JOIN t_product
ON t_order.productNo = t_product.id;

# 주문 테이블, 상품테이블, 회원테이블 붙이기
SELECT *
FROM t_order
INNER JOIN t_product
ON t_order.productNo = t_product.id
INNER JOIN t_user
ON t_order.userNo = t_user.id;


